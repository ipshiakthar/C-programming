//Write a program in C which takes input as an year and output as the year is leap year or not.
#include<stdio.h>
int main()
{
int year;
printf("Enter an year:");
scanf("%d",&year);
if(year%400==0)
{
printf("%d is a leap year",year);
}
else if(year%100==0)
{
printf("%d is not a leap year",year);
}
else if(year%4==0)
{
printf("%d is a leap year",year);
} else {
printf("%d is not a leap year",year);
}
return 0;
}
